{
  "script": [
    "ROOT/build/bin/doro",
    "JSON"
  ],
  "root path": "/media/gtnetuser/Dell/CDMA",
  "max process": 12,
  "seed": [
    "ITER_RANDOM",
    10000
  ],
  "A size": [
    "ITER_ALL",
    1000100,
    1001000,
    1003000,
    1010000
  ],
  "B size": [
    "ITER_WITH",
    "A size",
    {
      "1010000": [
        1010000,
        1030000,
        1100000,
        1300000
      ],
      "default": 1010000
    }
  ],
  "A minus B size": 10000,
  "k": 5,
  "diff": [
    "EVAL",
    "${B size} - ${A size} + 2 * ${A minus B size}"
  ],
  "df": [
    "ITER_WITH",
    "diff",
    {
      "10100": 10.5,
      "11000": 10.5,
      "13000": 11,
      "20000": 8.5,
      "40000": 8,
      "110000": 5.5,
      "310000": 3.2
    }
  ],
  "d": [
    "EVAL",
    "int(${diff} * ${df})"
  ],
  "max num peels": [
    "EVAL",
    "int(${diff} * 2.5)"
  ],
  "ta": 100,
  "max rounds": 1000,
  "max comm rounds": 10,
  "counting": true,
  "result filename": "ROOT/result/two_side_quantize2/bs{B size}_d{d}_sd{seed}.json"
}