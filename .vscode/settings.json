{
  "python.defaultInterpreterPath": "python3",
  "python.languageServer": "Pylance",
  "files.exclude": {
    "result": true
  },
  "files.watcherExclude": {
    "result": true
  },
  "search.exclude": {
    "result": true
  },
  "workbench.editor.limit.excludeDirty": false,
  "explorer.excludeGitIgnore": false,
  "C_Cpp.default.compilerPath": "/usr/bin/g++-13",
  "C_Cpp.vcFormat.newLine.beforeElse": false,
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "sameLine",
  "files.associations": {
    "*.txt": "makefile",
    "svd": "cpp",
    "*.inc": "cpp",
    "*.tcc": "cpp",
    "random": "cpp",
    "iosfwd": "cpp",
    "functional": "cpp",
    "valarray": "cpp",
    "iostream": "cpp",
    "unordered_map": "cpp",
    "ranges": "cpp",
    "chrono": "cpp",
    "limits": "cpp",
    "regex": "cpp",
    "type_traits": "cpp",
    "tuple": "cpp",
    "sparsetable": "cpp",
    "format": "cpp",
    "deque": "cpp",
    "vector": "cpp",
    "cctype": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "cstddef": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "any": "cpp",
    "array": "cpp",
    "bit": "cpp",
    "compare": "cpp",
    "concepts": "cpp",
    "cstdint": "cpp",
    "forward_list": "cpp",
    "map": "cpp",
    "string": "cpp",
    "exception": "cpp",
    "algorithm": "cpp",
    "iterator": "cpp",
    "memory": "cpp",
    "memory_resource": "cpp",
    "numeric": "cpp",
    "optional": "cpp",
    "string_view": "cpp",
    "utility": "cpp",
    "initializer_list": "cpp",
    "iomanip": "cpp",
    "istream": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "span": "cpp",
    "stdexcept": "cpp",
    "typeinfo": "cpp",
    "variant": "cpp",
    "charconv": "cpp",
    "complex": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "numbers": "cpp",
    "sstream": "cpp",
    "streambuf": "cpp",
    "system_error": "cpp",
    "text_encoding": "cpp"
  },
  "C_Cpp.errorSquiggles": "enabled",
  "cmake.debugConfig": {
    "args": [
      "${workspaceFolder}/config/threshold2.json"
    ]
  }
}